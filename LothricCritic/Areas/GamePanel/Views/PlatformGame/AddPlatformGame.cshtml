@model EntityLayer.Concrete.PlatformGame

@{
    ViewData["Title"] = "AddPlatformGame";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}




<div class="col-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Yeni PlatformGame Girişi</h4>
            <form class="forms-sample" method="post">
                @Html.Label("Şirket Adı")
                <br />
                <br />
                @Html.DropDownListFor(x=>x.PlatformID,(List<SelectListItem>)ViewBag.vp, new {@class="form-control"})
                <br />
                @Html.Label("Şirket Adı")
                <br />
                <br />
                @Html.DropDownListFor(x=>x.GameID,(List<SelectListItem>)ViewBag.vg, new {@class="form-control"})
                <br />
                <button type="submit" class="btn btn-primary me-2">Kaydet</button>
            </form>
        </div>
    </div>
</div>




